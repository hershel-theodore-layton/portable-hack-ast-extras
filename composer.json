{
  "name": "hershel-theodore-layton/portable-hack-ast-extras",
  "description": "Extra utilities for use with portable-hack-ast.",
  "license": "MIT",
  "require": {
    "hhvm": "^4.102",
    "hershel-theodore-layton/portable-hack-ast": "<1"
  },
  "require-dev": {
    "hhvm/hhvm-autoload": "^3.2",
    "hhvm/hhast": "^4.102",
    "hhvm/hacktest": "^2.3"
  },
  "scripts": {
    "post-install-cmd": [
      ".github/workflows/prepare-environment.sh"
    ],
    "post-update-cmd": [
      ".github/workflows/prepare-environment.sh"
    ]
  },
  "config": {
    "allow-plugins": {
      "hhvm/hhvm-autoload": true
    }
  }
}
